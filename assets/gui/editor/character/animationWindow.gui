Panel.AnimationPanel {
    Size = (ControlPanel.x, &.h);

    Renderer {
        BackgroundColor = #ABABAB;
        BorderColor = black;
        TextureBackground = None;
    }
}

Panel.ControlPanel {
    Position = (&.w - w, 0);
    Size = (300, &.h);

    Renderer {
        BackgroundColor = white;
        BorderColor = black;
        TextureBackground = None;
    }

    EditBox.ActionID {
        Position = (110, 10);
        Renderer = &1;
        Size = (42, 21);
        TextSize = 13;
    }

    Button.ActionSelect {
        Position = (160, 10);
        Renderer = &2;
        Size = (130, 21);
        TextSize = 13;
    }

    Label.BlockLabel {
        HorizontalAlignment = Right;
        Position = (0, 40);
        Renderer = &3;
        ScrollbarPolicy = Automatic;
        Size = (110, 17);
        Text = "animation.block";
        TextSize = 13;
    }

    SpinButton.BlockSpin {
        Maximum = 10;
        Minimum = 0;
        Position = (110, 36);
        Renderer = &4;
        Size = (42, 21);
        Step = 1;
        Value = 0;
    }

    Label.FrameLabel {
        HorizontalAlignment = Right;
        Position = (0, 60);
        Renderer = &3;
        ScrollbarPolicy = Automatic;
        Size = (110, 17);
        Text = "animation.frame";
        TextSize = 13;
    }

    SpinButton.FrameSpin {
        Maximum = 10;
        Minimum = 0;
        Position = (110, 56);
        Renderer = &4;
        Size = (42, 21);
        Step = 1;
        Value = 0;
    }

    Button.Play {
        Position = (160, 36);
        Renderer = &2;
        Size = (130, 21);
