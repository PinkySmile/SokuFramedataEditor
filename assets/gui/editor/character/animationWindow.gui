        InvertedDirection = false;
        Maximum = 10;
        Minimum = 0;
        Position = (20, 90);
        Size = (260, 16);
        Step = 1;
        Value = 0;

        Renderer {
            BorderColor = #3C3C3C;
            BorderColorHover = black;
            Borders = (1, 1, 1, 1);
            TextureThumb = None;
            TextureThumbHover = None;
            TextureTrack = None;
            TextureTrackHover = None;
            ThumbColor = #F5F5F5;
            ThumbColorHover = white;
            TrackColor = #F5F5F5;
